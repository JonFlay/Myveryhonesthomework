   "source": [
    "numba=int(input(\"Введи номер улицы, где живёт Фибоначи.\"))\n",
    "def fib(numba):\n",
    "    if numba == 0:\n",
    "        return 0\n",
    "    if numba == 1:\n",
    "        return 1\n",
    "    return fib(numba-1) + fib(numba-2)\n",
    "\n",
    "\n",
    "    return fib(numba)"
   ]
